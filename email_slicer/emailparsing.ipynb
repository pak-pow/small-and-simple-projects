    "import re \n",
    "\n",
    "with open(\"sample_data.txt\", \"r\", newline = \"\") as file: \n",
    "    reader = file.readlines()\n",
    "\n",
    "emails = [emails for lines in reader for emails in re.findall(r\"[\\w\\.-]+@[\\w\\.-]+\\.\\w+\", lines)] \n",
    "\n",
    "for e in emails:\n",
    "    print(e)"
